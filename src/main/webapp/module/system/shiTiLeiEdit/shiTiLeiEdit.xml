                  <property component="simpleProperty" name="ziDuanLXMC" label="字段类型名称" fieldType="string"/>
                </properties>
                <filters component="filters"/>
                <sorts component="sorts"/>
              </dataset>
            </column>
            <column fieldType="int" component="intColumn" label="字段长度" name="ziDuanCD" enabled="true" allowBlank="false" renderto="#ziDuanCD" renderTemplate="{{ziDuanCD}}" renderType="insert" textAlign="right" zeroAsBlank="true" width="40px"/>
            <column fieldType="int" component="intColumn" label="字段精度" name="ziDuanJD" enabled="true" allowBlank="true" renderto="#ziDuanJD" renderTemplate="{{ziDuanJD}}" renderType="insert" textAlign="right" zeroAsBlank="true" width="40px"/>
            <column fieldType="object" component="objectSelectColumn" label="关联实体类" name="guanLianSTL" enabled="true" allowBlank="true" renderto="#guanLianSTL" renderTemplate="{{guanLianSTL.shiTiLeiMC}}" renderType="insert" datasourceType="private" searchMode="local" minLength="0" width="100px" height="150px">
