                  <property component="simpleProperty" name="shiTiLeiMC" label="实体类名称" fieldType="string"/>
                </properties>
                <filters component="filters"/>
                <sorts component="sorts"/>
              </dataset>
            </column>
            <column fieldType="string" component="stringColumn" label="关联父类字段名称" name="guanLianFLZD" enabled="true" allowBlank="true" renderto="#guanLianFLZD" renderTemplate="{{guanLianFLZD}}" renderType="insert" width="80px"/>
