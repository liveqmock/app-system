      <field fieldType="boolean" component="booleanCheckEditor" name="shiFouSJKB" label="是否数据库表" renderType="insert" width="287px" height="22px" renderto="#shiFouSJKB" renderTemplate="{{shiFouSJKB}}" enabled="true" allowEdit="true" allowBlank="false" hidden="false"/>
